EVAL_TEMPERATURE=0.1

# 생성용 모델 (interview_generation_node)
GEN_MODEL=gpt-4o
GEN_TEMPERATURE=0.2


FINETUNE_MODEL=hf.co/rlawnsrb731/gemma3-final-skn4th:F16
